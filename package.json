{
  "name": "sami-food",
  "version": "1.0.1",
  "description": "Delivery website for food that you need ",
  "main": "server.js",
  "keywords": [
    "react",
    "express",
    "mongodb",
    "node",
    "mern",
    "firebase",
    "tailwind"
  ],
  "scripts": {
    "dev:client": "cd client && pnpm dev:client",
    "dev:server": "cd server && npm run dev",
    "build": "cd server && npm install && cd ../client && pnpm run build",
    "client": "pnpm run dev --prefix client",
    "start": "cd ./server && node index.js"
  },
  "author": "Adil Tahiri",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.0.1"
  }
}
